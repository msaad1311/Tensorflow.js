     const [r, g, b, a] = [imgData[i*4], imgData[i*4+1], imgData[i*4+2], imgData[i*4+3]];\r\n  \r\n      const gray = ((0.3 * r) + (0.59 * g) + (0.11 * b));\r\n      [\r\n        newImgData[i*4],\r\n        newImgData[i*4+1],\r\n        newImgData[i*4+2],\r\n        newImgData[i*4+3]\r\n      ] = !map[i] ? [gray, gray, gray, 255] : [r, g, b, a];\r\n    }\r\n    \r\n    \r\n    // Draw the new image back to canvas\r\n    ctx.putImageData(newImg, 0, 0);\r\n    \r\n  }\r\n  \r\nloadImage('./images/human.jpg');"},"lineCount":null}},"error":null,"hash":"beabefd5447a5419a150ec3ee78ecff8","cacheData":{"env":{}}}